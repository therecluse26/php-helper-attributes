{
    "name": "therecluse26/php-helper-attributes",
    "description": "Laravel package that adds simple, useful, drop-in helper attributes for memoization, caching, logging, timing, retries, transactions, and performance warnings",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "therecluse26",
            "email": "therecluse26@protonmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0",
        "illuminate/log": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "TheRecluse26\\LaravelAttributes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheRecluse26\\LaravelAttributes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheRecluse26\\LaravelAttributes\\LaravelAttributeServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
